    <!-- schema location for users of the SigiDoc's version of EFES available at https://github.com/SigiDoc/SigiDoc-->
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title xml:lang="en">[title of your seal record]</title>
                <title xml:lang="de">[Titel der Siegeldatei]</title>
                <editor xml:id="MC">Maria Teresa Catalano</editor>
                <editor xml:id="MF">Martina Filosa</editor>
                <editor xml:id="CS">Claudia Sode</editor>
                <!--here go the editors of the printed record, when available-->
            </titleStmt>
            <editionStmt>
                <edition>
                    <seg/>
                </edition>
                <editor ref="#MF"/>
                <editor xml:id="PE">Pia Evening</editor>
                <!-- here the editor of the digital edition should be defined. If he/she already appears above as editor of the seal, you can refer to them with a ref attribute and point to the xml:id with a #; if they appear for the first time, you should write a new xml:id as you did above on line 14 and 15. As an example I referred to MF as defined on l. 15 and added a new xml:id for, e.g., Pia-->
            </editionStmt>
            <publicationStmt>
                <authority>ANR/DFG DigiByzSeal - Unlocking the Hidden Value of Seals: New
                    Methodologies for Historical Research in Byzantine Studies</authority>
                <idno type="filename"/>
                <!--here goes the name of the file-->
                <idno type="SigiDocID"/>
                <!--SigiDocID should be ignored for now-->
                <idno type="sequence"/>
                <!--you can add the sequence number of this specific seal in your digital corpus. It is not compulsory however-->
                <date when="2023-03-24"/>
                <!--change the date according to what you need-->
                <availability>
                    <licence target="https://creativecommons.org/licenses/by-nc-sa/4.0/"
                        >Attribution-NonCommercial-ShareAlike 4.0 International - CC BY-NC-SA
                        4.0</licence>
                </availability>
            </publicationStmt>
            <sourceDesc>
                <listPerson  type="issuer">
                    <person gender="M" role="civil"> <!--gender to be changed as wished; the value of role is imperial civil military aristocracy secular-church monastic civic corporate private undetermined-->
                        <persName xml:lang="grc">
                            <forname/>
                            <surname/>
                        </persName>
                        <persName xml:lang="en">
                            <forname/>
                            <surname/>
                        </persName>
                        <persName xml:lang="de">
                            <forname/>
                            <surname/>
                        </persName>
                        <idno type="PBW" ana="http://pbw2016.kdl.kcl.ac.uk/person/Basileios/20221/">PBW Basileios 20221</idno>
                        <idno type="PmbZ"/>
                        <idno type="PLP"/>
                        <idno type="PLRE"/>
                    </person>
                </listPerson>
                <msDesc>
                    <msIdentifier>
                        <country>
                            <seg xml:lang="en">Germany</seg>
                            <seg xml:lang="de">Deutschland</seg>
                        </country>
                        <settlement>
                            <seg xml:lang="en">Cologne</seg>
                            <seg xml:lang="de">KÃ¶ln</seg>
                        </settlement>
                        <institution/>
                        <repository/>
                        <collection xml:lang="en">Robert Feind Collection</collection>
                        <collection xml:lang="de">Sammlung Robert Feind</collection>
                        <idno>XXX</idno>
                        <!--to be completed with the actual idno-->
                        <altIdentifier>
                            <repository/>
                            <!--to be used when we have an attested older repository -->
                            <idno/>
                        </altIdentifier>
                    </msIdentifier>
                    <msContents>
                        <summary n="whole">
                            <seg xml:lang="en"/>
                            <seg xml:lang="de"/>
                        </summary>
                    </msContents>
                    <physDesc>
                        <objectDesc>
                            <supportDesc>
                                <support>
                                    <objectType>
                                        <term>
                                            <seg xml:lang="en">Seal</seg>
                                            <seg xml:lang="de">Siegel</seg>
                                        </term>
                                        <interp type="workType" xml:lang="en">Original
                                            impression</interp>
                                        <interp type="workType" xml:lang="de">Origineller
                                            Siegelabdruck</interp>
                                    </objectType>
                                    <material>
                                        <seg xml:lang="en">Lead</seg>
                                        <seg xml:lang="de">Blei</seg>
                                    </material>
                                    <dimensions type="support" unit="mm">
                                        <dim type="diameter"/>
                                    </dimensions>
                                    <measure type="weight" unit="g"/>
                                    <measure type="axis" unit="clock"/>
                                    <measure type="overstrikeOrient" unit="clock"/>
                                    <measure type="channelOrient" unit="clock"/>
                                    <measure type="countermark" unit="clock"/>
                                </support>
                                <condition rend="">
                                    <p xml:lang="en"/>
                                    <p xml:lang="de"/>
                                </condition>
                            </supportDesc>
                            <layoutDesc>
                                <layout n="whole">
                                    <rs type="layout">
                                        <seg xml:lang="en"/>
                                        <seg xml:lang="de"/>
                                    </rs>
                                    <rs type="execution">
                                        <seg xml:lang="en">Struck</seg>
                                        <seg xml:lang="de">???</seg>
                                    </rs>
                                    <rs type="shape">
                                        <seg xml:lang="en">Round</seg>
                                        <seg xml:lang="de">Rund</seg>
                                    </rs>
                                    <rs type="matrix" subtype="not-surviving">
                                        <idno type="SigiDoc"/>
                                        <!--not to be used yet-->
                                        <idno type="PBW" corresp="URI"/>
                                        <!--to be used if the seal has a parallel in the PBW with a boulloterion registered there-->
                                    </rs>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
                        <decoDesc>
                            <decoNote>
                                <figure>
                                    <figDesc n="whole" xml:lang="en"/>
                                    <!--only some keywords regarding the iconography, if applicable, e.g. Saint Theodore standing, Bust of the Mother of God -->
                                    <figDesc n="whole" xml:lang="de"/>
                                </figure>
                            </decoNote>
                        </decoDesc>
                    </physDesc>
                    <history>
                        <origin>
                            <origPlace>
                                <seg xml:lang="en">
                                    <placeName ref="URI"/>
                                </seg>
                                <seg xml:lang="de">
                                    <placeName ref="URI"/>
                                </seg>
                            </origPlace>
                            <!-- for a precise year (WHEN):-->
                            <!--remember: you will most probably only need one of these. Whatever you DON'T need goes in a green comment-->
                            <!--<origDate type="analysis" when="0001" evidence="" cert="" resp="">
                                <seg xml:lang="en"/>
                                <seg xml:lang="de"/>
                                <interp type="datingCriteria" xml:lang="en"/>
                                <interp type="datingCriteria" xml:lang="de"/>
                            </origDate>-->
                            <!-- for a date range or centuries (NOT BEFORE - NOT AFTER)-->
                            <origDate type="analysis" notBefore="0001" notAfter="2023" evidence="" cert=""
                                resp="">
                                <seg xml:lang="en"/>
                                <seg xml:lang="de"/>
                                <interp type="datingCriteria" xml:lang="en"/>
                                <interp type="datingCriteria" xml:lang="de"/>
                            </origDate>
                            <!-- for an internal date -->
                            <!--<origDate type="internal" when="0001" when-custom="2023" datingMethod="">
                                <seg xml:lang="en"/>
                                <seg xml:lang="de"/>
                                <interp type="datingCriteria" xml:lang="en"/>
                                <interp type="datingCriteria" xml:lang="de"/>
                            </origDate>-->
                            <!-- for erroneous dating and corrections: -->
                            <!--<date>
                                <choice>
                                    <sic resp=""/>
                                    <corr resp="" evidence="" cert=""/>
                                </choice>
                                <interp type="datingCriteria" xml:lang=""/>
                            </date>-->
                        </origin>
                        <provenance type="found" subtype="" when="2023" xml:lang="" source="" cert="">
                            <placeName type="ancientFindspot" ref="URI"/>
                            <placeName type="modernFindspot" ref="URI"/>
                            <rs type="circumstances"/>
                        </provenance>
                        <provenance type="observed" subtype="">
                            <p xml:lang="en"/>
                            <p xml:lang="de"/>
                        </provenance>
                        <provenance type="not-observed" subtype="">
                            <p xml:lang="en"/>
                            <p xml:lang="de"/>
                        </provenance>
                        <provenance type="transferred" subtype="">
                            <p xml:lang="en"/>
                            <p xml:lang="de"/>
                        </provenance>
                        <acquisition>
                            <p xml:lang="en"/>
                            <p xml:lang="de"/>
                        </acquisition>
                    </history>
                    <msPart n="r">
                        <msIdentifier>
                            <idno/>
                        </msIdentifier>
                        <msContents>
                            <summary n="r">
                                <seg>Obverse</seg>
                            </summary>
                            <textLang n="r" mainLang="grc">
                                <!--if the language changes, also the value of mainLang should change accordingly-->
                                <seg xml:lang="en">Byzantine Greek</seg>
                                <seg xml:lang="fr">Griechisch</seg>
                            </textLang>
                        </msContents>
                        <physDesc>
                            <objectDesc>
                                <layoutDesc>
                                    <layout n="r">
                                        <dimensions type="written" unit="mm">
                                            <dim type="diameter"/>
                                        </dimensions>
                                        <rs type="matrix" subtype="not-surviving">
                                            <idno type="SigiDoc"/>
                                            <!--not to be used yet-->
                                        </rs>
                                        <rs type="layout">
                                            <seg xml:lang="en"/>
                                            <seg xml:lang="de"/>
                                        </rs>
                                        <p xml:lang="en"/>
                                        <p xml:lang="de"/>
                                    </layout>
                                </layoutDesc>
                            </objectDesc>
                            <handDesc>
                                <handNote n="r">
                                    <!--NB: in this field you can refer to epigraphic features on the seal using also AthenaRuby-->
                                    <seg xml:lang="en"/>
                                    <seg xml:lang="de"/>
                                </handNote>
                            </handDesc>
                            <decoDesc>
                                <decoNote>
                                    <figure>
                                        <figDesc n="r" xml:lang="en"/>
                                        <figDesc n="r" xml:lang="de"/>
                                        <figDesc n="decoR" xml:lang="en"/>
                                        <figDesc n="decoR" xml:lang="de"/>
                                    </figure>
                                </decoNote>
                            </decoDesc>
                        </physDesc>
                        <history>
                            <origin>
                                <origDate type="analysis">
                                    <!--only for assembled boulloteria-->
                                    <seg/>
                                    <interp type="datingCriteria"/>
                                </origDate>
                                <date/>
                            </origin>
                        </history>
                    </msPart>
                    <msPart n="v">
                        <msIdentifier>
                            <idno/>
                        </msIdentifier>
                        <msContents>
                            <summary n="v">
                                <seg>Reverse</seg>
                            </summary>
                            <textLang n="v" mainLang="grc">
                                <!--if the language changes, also the value of mainLang should change accordingly-->
                                <seg xml:lang="en">Byzantine Greek</seg>
                                <seg xml:lang="de">Griechisch</seg>
                            </textLang>
                        </msContents>
                        <physDesc>
                            <objectDesc>
                                <layoutDesc>
                                    <layout n="v">
                                        <dimensions type="written" unit="mm">
                                            <dim type="diameter"/>
