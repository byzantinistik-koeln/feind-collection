                                            <idno type="SigiDoc"/>
                                            <!--not to be used yet-->
                                        </rs>
                                        <rs type="layout">
                                            <seg xml:lang="en"/>
                                            <seg xml:lang="de"/>
                                        </rs>
                                        <p xml:lang="en"/>
                                        <p xml:lang="de"/>
