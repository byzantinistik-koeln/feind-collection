            <div type="edition" subtype="editorial" xml:space="preserve"> 
                  <div type="textpart" subtype="label" n="obv" rend="columns" xml:lang="grc"> <!--obverse, editorial-->
                      <ab>
                          <cb n="a"/><expan><ex>Ὁ ἅγιος</ex></expan> <expan><abbr>Παντ</abbr><ex>ε</ex>
                          <cb n="b" break="no"/><abbr>λε<supplied reason="lost">ή</supplied>μ</abbr><ex>ων</ex></expan>
